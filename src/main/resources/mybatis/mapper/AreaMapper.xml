<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springbootmybatis.mapper.AreaMapper">
    <select id="getAll" resultType="com.springbootmybatis.model.AreaEntity">
        select * from wemall_area
    </select>

    <select id="getOne" parameterType="long" resultType="com.springbootmybatis.model.AreaEntity">
        select * from wemall_area where id=#{id}
    </select>

</mapper>